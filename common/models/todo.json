{
  "name": "todo",
  //"forceId": false,
  "plural": "todos",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "id": {
      "type": "number",
      "generated": true,
      "id": true
    },
    "description": {
      "type": "string",
      "required": true
    },
    "priority": {
      "type": "number",
      "required": true
    },
    "status": {
      "type": "string",
      "required": true
    },
    "created": {
      "type": "date",
      "required": true,
      "default": "$now"
    }
  },
  "validations": [],
  "relations": {
    "categories": {
      "type": "hasAndBelongsToMany",
      "model": "category"
    }
  },
  "acls": [],
  "methods": {}
}
